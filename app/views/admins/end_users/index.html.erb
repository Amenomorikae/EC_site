<div class="container">
    <div class="row col-md-8 col-md-offset-1">
        <h2>会員一覧画面</h2>
    </div>


    <div class="row col-md-8 col-md-offset-2">
            <table class="table">
                <thead>
                    <tr>
                        <th>会員ID</th>
                        <th>氏名</th>
                        <th>メールアドレス</th>
                        <th>ステータス</th>
                    </tr>
                </thead>
                <tbody>
                    <% @end_users.each do |end_user| %>
                        
                    <tr>
                        <td><%= end_user.id %></td>
                        <td><%= link_to end_user.surname + " " + end_user.name, admins_end_user_path(end_user.id) %></td>
                        <td><%= end_user.email %></td>
                        <td><%= end_user.is_deleted %></td>
                    </tr>
                    <% end %>
                </tbody>
            </table>
    </div>
</div>
